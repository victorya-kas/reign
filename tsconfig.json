{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["./components/*"],
      "@layouts/*": ["./layouts/*"],
      "@pages/*": ["./pages/*"],
      "@assets/*": ["./assets/*"],
      "@ts/*": ["./ts/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
